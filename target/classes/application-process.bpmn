        <bpmndi:BPMNLabel>
          <dc:Bounds x="683" y="105" width="75" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_HandleRejection_di" bpmnElement="Task_HandleRejection">
        <dc:Bounds x="820" y="320" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_DocumentVerification_di" bpmnElement="Task_DocumentVerification">
        <dc:Bounds x="820" y="60" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_DocumentDecision_di" bpmnElement="Gateway_DocumentDecision" isMarkerVisible="true">
        <dc:Bounds x="995" y="75" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="983" y="45" width="75" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_HandleIncompleteDocuments_di" bpmnElement="Task_HandleIncompleteDocuments">
        <dc:Bounds x="970" y="200" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_CompleteDocumentVerification_di" bpmnElement="Task_CompleteDocumentVerification">
        <dc:Bounds x="1120" y="60" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Event_ApplicationRejected_di" bpmnElement="Event_ApplicationRejected">
        <dc:Bounds x="1002" y="342" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="988" y="385" width="64" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Event_ApplicationSuccessful_di" bpmnElement="Event_ApplicationSuccessful">
        <dc:Bounds x="1302" y="82" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1288" y="125" width="64" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="Flow_ToSaveApplication_di" bpmnElement="Flow_ToSaveApplication">
        <di:waypoint x="188" y="160" />
        <di:waypoint x="240" y="160" />
      </bpmndi:BPMNEdge>
